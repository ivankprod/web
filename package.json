{
	"name": "ivankprod-web",
	"version": "1.0.0",
	"private": true,
	"description": "ivankprod website",
	"author": "ivankprod",
	"license": "MIT",
	"keywords": [
		"gatsby",
		"ivankprod",
		"ivank production"
	],
	"browserslist": [
		"since 2010",
		"not op_mini all",
		"not ie >= 6"
	],
	"scripts": {
		"develop": "gatsby develop",
		"start": "gatsby develop",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"typecheck": "tsc --noEmit",
		"lint": "npm run lint:es && npm run lint:css",
		"lint:es": "eslint src/** --config .eslintrc.js --no-error-on-unmatched-pattern",
		"lint:css": "stylelint src/** --config .stylelintrc",
		"gen:component": "generate-react component",
		"gen:element": "generate-react component --type=element",
		"gen:form": "generate-react component --type=form",
		"gen:page": "generate-react component --type=page"
	},
	"dependencies": {
		"@fontsource/open-sans": "^4.5.13",
		"autoprefixer": "^10.4.8",
		"clsx": "^1.2.1",
		"gatsby": "^4.24.8",
		"gatsby-plugin-image": "^2.17.0",
		"gatsby-plugin-manifest": "^4.25.0",
		"gatsby-plugin-netlify": "^5.1.0",
		"gatsby-plugin-postcss": "^5.24.0",
		"gatsby-plugin-react-helmet": "^5.24.0",
		"gatsby-plugin-sharp": "^4.25.0",
		"gatsby-plugin-sitemap": "^5.25.0",
		"gatsby-source-filesystem": "^4.25.0",
		"gatsby-transformer-json": "^4.25.0",
		"gatsby-transformer-sharp": "^4.25.0",
		"normalize.css": "^8.0.1",
		"postcss": "^8.4.21",
		"postcss-preset-env": "^8.0.1",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-helmet": "^6.1.0",
		"react-jss": "^10.9.2"
	},
	"devDependencies": {
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.9",
		"@types/react-helmet": "^6.1.6",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.50.0",
		"eslint": "^8.33.0",
		"eslint-plugin-react": "^7.31.11",
		"generate-react-cli": "^8.0.1",
		"stylelint": "^14.16.1",
		"stylelint-config-standard": "^29.0.0",
		"typescript": "^4.9.3"
	}
}
