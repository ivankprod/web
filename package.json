{
	"name": "ivankprod-web",
	"version": "1.0.0",
	"private": true,
	"description": "ivankprod website",
	"author": "ivankprod",
	"license": "MIT",
	"keywords": [
		"gatsby",
		"ivankprod",
		"ivank production"
	],
	"browserslist": [
		"since 2013",
		"not op_mini all",
		"not ie >= 6"
	],
	"scripts": {
		"develop": "gatsby develop",
		"start": "gatsby develop",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"typecheck": "tsc --noEmit",
		"lint": "npm run lint:es && npm run lint:css",
		"lint:es": "eslint src/** --config .eslintrc.js --no-error-on-unmatched-pattern",
		"lint:css": "stylelint src/** --config .stylelintrc",
		"gen:component": "generate-react component",
		"gen:element": "generate-react component --type=element",
		"gen:form": "generate-react component --type=form",
		"gen:page": "generate-react component --type=page"
	},
	"dependencies": {
		"@fontsource/open-sans": "^4.5.14",
		"autoprefixer": "^10.4.14",
		"clsx": "^1.2.1",
		"gatsby": "^5.7.0",
		"gatsby-plugin-image": "^3.7.0",
		"gatsby-plugin-manifest": "^5.7.0",
		"gatsby-plugin-netlify": "^5.1.0",
		"gatsby-plugin-postcss": "^6.7.0",
		"gatsby-plugin-react-helmet": "^6.7.0",
		"gatsby-plugin-sharp": "^5.8.1",
		"gatsby-plugin-sitemap": "^6.7.0",
		"gatsby-source-filesystem": "^5.8.0",
		"gatsby-transformer-json": "^5.7.0",
		"gatsby-transformer-remark": "^6.7.0",
		"gatsby-transformer-sharp": "^5.7.0",
		"normalize.css": "^8.0.1",
		"postcss": "^8.4.21",
		"postcss-preset-env": "^8.3.2",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-helmet": "^6.1.0",
		"react-jss": "^10.10.0"
	},
	"devDependencies": {
		"@types/node": "^18.15.11",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.11",
		"@types/react-helmet": "^6.1.6",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.0",
		"eslint": "^8.38.0",
		"eslint-plugin-react": "^7.32.2",
		"generate-react-cli": "^8.2.0",
		"stylelint": "^15.2.0",
		"stylelint-config-standard": "^30.0.1",
		"typescript": "^4.9.5"
	}
}
